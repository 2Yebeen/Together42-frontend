{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "lib": [
      "ES2020",
      "DOM"
    ],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "allowJs": true,
    "paths": {
      "@main/*": [
        "src/components/Main/*"
      ],
      "@result/*": [
        "src/components/Result/*"
      ],
      "@auth/*": [
        "src/components/Auth/*"
      ],
      "@review/*": [
        "src/components/Review/*"
      ],
      "@utils/*": [
        "src/components/utils/*"
      ],
      "@recoil/*": [
        "src/recoil/*"
      ],
      "@img/*": [
        "src/assets/img/*"
      ],
      "@css/*": [
        "src/assets/css/*"
      ],
      "@usefulObj/*": [
        "src/assets/usefulObj/*"
      ],
      "@types": [
        "src/assets/usefulObj/types"
      ],
      "@cert/*": [
        "src/cert/*"
      ],
    }
  }
}